{
  "version": 2,
  "rootDirectory": "examples/express-vercel-mcp-server",
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/mcp",
      "dest": "dist/index.js",
      "methods": [
        "POST"
      ]
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ],
  "env": {
    "PORT": "3000"
  }
}